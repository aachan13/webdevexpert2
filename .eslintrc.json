{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "no-console": 0,
        "linebreak-style": 0,
        "import/no-extraneous-dependencies": 0,
        "no-trailing-spaces": 0,
        "eol-last": 0,
        "no-extra-semi": 0,
        "no-underscore-dangle": 0,
        "no-multiple-empty-lines": 0,
        "no-restricted-globals": 0
    }
}
